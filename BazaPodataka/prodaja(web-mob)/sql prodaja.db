BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "Korpa" (
	"IDKorpa"	INTEGER,
	"Sifra"	NUMERIC,
	"Cena"	NUMERIC,
	"IDProizvod"	TEXT,
	PRIMARY KEY("IDKorpa" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "Lista_zelja" (
	"IDListaZelja"	INTEGER,
	"ImeProizvoda"	TEXT,
	"Cena"	NUMERIC,
	PRIMARY KEY("IDListaZelja" AUTOINCREMENT)
);
COMMIT;
