program GatekeeperWeb;

uses
  System.StartUpCopy,
  FMX.Forms,
  LogIn in 'LogIn.pas' {frmLogIn},
  Register in 'Register.pas' {frmRegister};

{$R *.res}

begin
  Application.Initialize;
  Application.CreateForm(TfrmLogIn, frmLogIn);
  Application.CreateForm(TfrmRegister, frmRegister);
  Application.Run;
end.
